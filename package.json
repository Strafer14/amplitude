    "amplitude"
  ],
  "dependencies": {
    "superagent": "^5.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/eslint": "^6.1.8",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@types/superagent": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "ecmascript-version-detector": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "nock": "^11.8.2",
    "nyc": "^15.0.0",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "standard": "^14.3.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "license": "ISC",
  "greenkeeper": {
    "ignore": [
      "nock"
    ]
  },
  "mocha": {
    "colors": true,
    "reporter": "spec",
    "timeout": 8000,
    "growl": true,
    "check-leaks": true,
    "recursive": true,
    "require": "ts-node/register",
    "extension": [
      "ts",
      "js"
    ]
  }
