  "scripts": {
    "build": "rm -rf dist && tsc",
    "contributors": "node tasks/generate-contributors-list",
    "coverage": "codecov",
    "lint": "tsc --noEmit && eslint '**/*.{js,ts}'",
    "test": "npm run lint && nyc --reporter=lcov --reporter=text mocha",
    "tdd": "mocha --watch"
  },
  "main": "dist/amplitude.js",
  "types": "dist/amplitude.d.ts",
  "repository": {
    "type": "git",
